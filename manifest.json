{
  "manifest_version": 3,
  "name": "Guiqin ",
  "description": "Guiqin (桂琴) is a Google Chrome extension for checking the learning status of Chinese words and their basic statistics.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "contextMenus",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}